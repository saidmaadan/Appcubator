<div class='container'>
<div class='row'>
<div class='row'>

<div class='col-md-10'>
	
</div>
<div class='col-md-2'>
<div class = 'btn btn-info'><h4><%= pluralize @users.size, 'User' %></h4></div><br><br><br>
  
</div></div>
<% if @users.any? %>
<% @users.in_groups_of(3) do |group| %>
<div class='row'>
<% group.each do |user| %>
<% if user %>
<div class='col-md-4'>
<div class='row'>
<div class='col-md-6'>
     <p><%= profile_image_for(user) %></p></div>
  <div class='col-md-6'>
      <h4><%= link_to user.name, user %></h4></div>
</div><br>
</div>
<% end %>
<% end %>
</div>
<% end %>
<% end %>





 
</div>
</div>
