<div class='container'>
<div class='row'>
<div class='row'>

<div class='col-md-10'>
	
</div>
<div class='col-md-2'>
<div class = 'btn btn-default'><h4><%= pluralize @users.size, 'Member' %></h4></div><br><br><br>
  
</div></div>
<% if @users.any? %>
<% @users.in_groups_of(4) do |group| %>
<div class='row'>
<% group.each do |user| %>
<% if user %>
<div class='col-md-3'>
<div class='row'>
<div class='col-md-4'>
     <p><%= profile_image_for(user) %></p></div>
  <div class='col-md-8'>
      <h4><%= link_to user.name, user %></h4>
      <h5><%= user.specialization %></h5>
      <h5><%= user.location %></h5>
      </div>
</div><br>
</div>
<% end %>
<% end %>
</div>
<% end %>
<% end %>





 
</div>
</div>
