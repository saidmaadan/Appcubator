<div class='container'>
<div class='row'>
<div class='row'>

<div class='col-md-10'>
	
</div>
<div class='col-md-2'>
<div ><h4><%#= pluralize @users.size, 'Member' %></h4></div><br><br><br>
  
</div></div>
<% if @users.any? %>
	<div class='row'>
		<% @users.each do |user| %>
			<% if user %>
				<div class='col-md-3 col-sm-4 col-xs-6' >
					<div class='row'>
						<div class='col-sm-5 col-xs-12'>
						    <p><%= profile_image_for user, class: 'img-responsive', style: "width:100%;height:150px;" %></p>
						</div>
				  	<div class='col-sm-7 col-xs-12'>
				      <h4><%= link_to user.name, user %></h4>
				      <h5><%= user.specialization %></h5>
				      <h5><%= user.location %></h5>
				  	</div>
					</div><br>
				</div>
			<% end %>
		<% end %>
	</div>

<% end %>

 <div class='paginate'>
    <%= paginate @users, :outer_window => 3 %>
</div>



 
</div>
</div>
